<IfModule mod_rewrite.c>
   RewriteEngine On
   RewriteRule ^view/  -  [L]
   RewriteRule ^tmp/images/  -  [L]
   RewriteRule ^(.*)/(.*)$ index.php?page=$1&id=$2&%{QUERY_STRING} [L]
   RewriteRule ^(.*)/$ index.php?page=$1&%{QUERY_STRING} [L]
   RewriteRule ^([^/.]+)$ index.php?page=$1&%{QUERY_STRING} [L]
</IfModule>

<IfModule mod_expires.c>
   ExpiresActive On
   ExpiresDefault "access plus 1 week"
   <FilesMatch "\.(php|cgi)$">
      ExpiresActive Off
   </FilesMatch>
</IfModule>